<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Nullable>enable</Nullable>
    <OutputType>Library</OutputType>
    <PackageId>Handy.ServirtiumPOC.AspNetCore</PackageId>
    <VersionPrefix>0.0.1</VersionPrefix>
    <VersionSuffix>dev.2</VersionSuffix>
    <PackageProjectUrl>https://github.com/stephenhand/servirtium-dotnet</PackageProjectUrl>
    <Authors>Stephen Hand</Authors>
    <Company />
    <Product>Servirtium</Product>
    <Description>Prototype of a .NET Core Servirtium implementation, this is a personal project, NOT an official Servirtium package.</Description>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
    <ProjectReference Include="..\Servirtium.Core\Servirtium.Core.csproj" />
  </ItemGroup>


</Project>
